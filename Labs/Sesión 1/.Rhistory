stock_prices2 <- "XLK" %>%
tq_get(get  = "stock.prices",
from = "2010-01-01",
to   = "2015-12-31") %>%
tq_transmute(select     = adjusted,
mutate_fun = periodReturn,
period     = "monthly",
col_rename = "Rb")
RaRb <- left_join(stock_prices1.1,
stock_prices2,
by = c("date" = "date"))
stocks_capm <-  RaRb %>%
tq_performance(Ra = stock_prices1.1,
Rb = stock_prices2,
performance_fun = table.CAPM)
