install.packages("tidyquant")
library(tidyquant)
library(xts)
library(tidyquant)
tq_transmute()
?tq_transmute()
? tq_mutate_fun_options()
tq_mutate_fun_options()
stock_prices1 <- c("AAPL", "GOOG", "NFLX") %>%
tq_get(get  = "stock.prices",
from = "2010-01-01",
to   = "2015-12-31")
stock_prices1.1 <- c("AAPL", "GOOG", "NFLX") %>%
tq_get(get  = "stock.prices",
from = "2010-01-01",
to   = "2015-12-31") %>%
group_by(symbol) %>%
tq_transmute(select     = adjusted,
mutate_fun = periodReturn,
period     = "monthly",
col_rename = "Ra")
library(dplyr)
stock_prices1.1 <- c("AAPL", "GOOG", "NFLX") %>%
tq_get(get  = "stock.prices",
from = "2010-01-01",
